[Unit]
Description=Prometheus
After=network.target

[Service]
User=prometheus
Group=prometheus
Type=simple
ExecStart=/home/prometheus/prometheus-2.24.0-rc.0.linux-armv7/prometheus\
  --config.file="/home/prometheus/prometheus-2.24.0-rc.0.linux-armv7/prometheus.yml"\
  --storage.tsdb.path="/home/prometheus/data"

[Install]
WantedBy=multi-user.target
